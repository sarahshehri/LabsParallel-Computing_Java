
	//Write, compile and run a program which does a matrix multiplication
  
  public static void main(String[] args) {

		//creating two matrices a two dimensional b one dimensional 
		double a[][]={{1,1,1},{2,2,2},{3,3,3}};    
		double b[]={1,2,3}; 
		
		//creating another matrix to store the multiplication of two matrices   
		double c[]=new double[3];
		
  
		//multiplying and printing multiplication of 2 matrices      
		for(int j=0;j<3;j++){    
		c[j]=0;      
		for(int k=0;k<3;k++)      
		{ 
			c[j]+= a[j][k]* b[k]; 
			
		}//end of k loop  
		System.out.print(c[j]+" ");  //printing matrix element  
		}//end of j loop  
		System.out.println();//new line    
		}    
    
    //Write, compile and run a program which does the pi computation n=100000
    
    public static void main(String[] args) {
                //initialise the variables pi ,factor
		double pi=0;
		double factor=1.0;
		//Computing pi value 
		for(int i=0; i<100000;i++) {
			pi+= factor/(2*i+1);
			factor=-1*factor;
		}
		//final operation for computing pi
		pi*=4.0;
		System.out.println(pi);
	}
	
	//1. Write a program that does the following:(a) Compute the average of array elements.(b) Sum two numbers, stored in a and b.
	
		public static void main(String []args) {
			double storVal=0;
			//create array 
			double []calculateAve= new double[46];
			//loop to fill array index 
			for(int i=0;i< calculateAve.length;i++) {
				calculateAve[i] =i+7;
				//variable for store the sum of indexes
				 storVal += calculateAve[i];
				
			}
	         //calculate the average 
	         double average= storVal/(calculateAve.length);
	         System.out.println("The average of array elements= "+average);
	         
	         //Sum two numbers, stored in a and b
	         int a=5;
	         int b=90;
	         int sum= a+b;
	         System.out.print("The sum of two numbers= "+sum);
		
	         
		} 
